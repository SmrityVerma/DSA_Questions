
int maxProfit(vector<int>& p) {
        
        int maxprofit=0,minprice=p[0];
        for(int i=0;i<p.size();i++)
        {
            minprice=min(minprice,p[i]);
            int price=p[i]-minprice;
            maxprofit=max(price,maxprofit);
        }
        
        return maxprofit;
    }
